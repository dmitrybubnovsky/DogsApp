<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.andersen.dogsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ActivityTwo">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>

                <action android:name="com.andersen.dogsapp.CUSTOM_ACTION"/>
            </intent-filter>
        </activity>
        <activity android:name=".ActivityThree">
        </activity>
        <activity android:name=".ActTextViewInFrameLayout">
        </activity>
        <activity android:name=".ActTextViewInRelativeLayout">
        </activity>
        <activity android:name=".ActTextViewInConstLayout">
        </activity>
        <activity android:name=".dogs.ActDogOwnersList"
                  android:theme = "@style/MainDogApp"/>

        <activity android:name=".dogs.ActOwnersDog"
                  android:theme = "@style/MainDogApp"/>
        <activity android:name=".dogs.ActDogsInfo"
                  android:theme = "@style/MainDogApp"/>
        <activity android:name=".dogs.ActMainDogSubscription"
                  android:theme = "@style/NoActionBar"/>
    </application>

</manifest>